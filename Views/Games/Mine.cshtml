@model IEnumerable<GameListingServiceModel>

@{
    ViewBag.Title = "My Games";
    ViewBag.AllowCarEdit = true;
}

@if (Model.Any() == false)
{
    <div class="col-md-4">
        <div class="card">
            <img class="card-img-top" src="https://thumbs.dreamstime.com/b/comic-boom-oops-icon-simple-black-style-illustration-web-122999699.jpg" alt="Just oops image">
            <div class="card-body text-center">
                <h5 class="card-title text-center">You do not have games</h5>
                <a href="/Games/Create" class="btn">Create Game</a>
            </div>
        </div>
    </div>
}

<partial name="_GamesPartial" model="@Model" />