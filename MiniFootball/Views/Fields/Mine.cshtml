@model IEnumerable<FieldServiceModel>

@{
    ViewBag.Title = "My Fields";
    ViewBag.AllowCarEdit = true;
}

@if (Model.Any() == false)
{
    <div class="my-item-center mt-5">
        <a href="/Fields/Create" class="btn min-width-size-btn">Create Field</a>
    </div>
}
else
{
    <div class="row">
        <partial name="_FieldPartial" model="@Model" />
    </div>
}
